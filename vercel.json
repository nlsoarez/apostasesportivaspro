{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api/**/*.py": {
      "maxDuration": 20,
      "memory": 512
    }
  },
  "regions": ["gru1"],
  "github": {
    "enabled": true,
    "autoJobCancelation": true
  }
}

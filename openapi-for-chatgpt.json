    "/analysis/corners": {
      "get": {
        "summary": "An\u00e1lise de escanteios",
        "description": "Calcula estimativa de escanteios baseado em:\n- M\u00e9dia de escanteios a favor (casa)\n- M\u00e9dia de escanteios a favor (fora)\n- Gera sugest\u00f5es de apostas com confian\u00e7a\n",
        "operationId": "getAnalysisCorners",
        "tags": [
          "analysis"
        ],
        "parameters": [
          {
            "name": "team_home",
            "in": "query",
            "required": true,
            "description": "ID do time mandante",
            "schema": {
              "type": "integer",
              "example": 127
            }
          },
          {
            "name": "team_away",
            "in": "query",
            "required": true,
            "description": "ID do time visitante",
            "schema": {
              "type": "integer",
              "example": 121
            }
          },
          {
            "name": "league",
            "in": "query",
            "required": true,
            "description": "ID da liga",
            "schema": {
              "type": "integer",
              "example": 71
            }
          },
          {
            "name": "season",
            "in": "query",
            "required": false,
            "description": "Ano da temporada",
            "schema": {
              "type": "integer",
              "default": 2025,
              "example": 2025
            }
          }
        ],
        "responses": {
          "200": {
            "description": "An\u00e1lise de escanteios gerada",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean",
                      "example": true
                    },
                    "analise_escanteios": {
                      "type": "object",
                      "properties": {
                        "time_casa": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "integer",
                              "example": 127
                            }
                          }
                        },

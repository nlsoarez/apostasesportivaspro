                                "example": "Over 9.5 Escanteios Totais"
                              },
                              "confianca": {
                                "type": "integer",
                                "minimum": 1,
                                "maximum": 5,
                                "example": 5
                              },
                              "value_estimado": {
                                "type": "string",
                                "example": "+20%"
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {

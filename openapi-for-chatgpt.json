                      "type": "boolean",
                      "example": true
                    },
                    "name": {
                      "type": "string",
                      "example": "Apostas Esportivas Pro API"
                    },
                    "version": {
                      "type": "string",
                      "example": "5.0"
                    },
                    "description": {
                      "type": "string"
                    },
                    "endpoints": {
                      "type": "object"
                    }
